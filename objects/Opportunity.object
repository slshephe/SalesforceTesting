    <validationRules>
        <fullName>ClosedDateAndFINRADateBothCanNotExists</fullName>
        <active>true</active>
        <description>ClosedDateAndFINRADateBothCanNotExists</description>
        <errorConditionFormula>NOT(ISBLANK(  Closed_Date__c ))  &amp;&amp;   NOT(ISBLANK(   FINRA_Effective_Date__c  ))</errorConditionFormula>
        <errorMessage>Closed Date and  FINRA Date  both can not contain values at the same time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LeadSourceDetail_ReadOnly</fullName>
        <active>false</active>
        <description>Based on LeadSource Picklist value leadSourceDetail text field Should be Read Only.</description>
        <errorConditionFormula>AND(
NOT(OR(
ISPICKVAL( LeadSource , &quot;Third Party Recruiter&quot;),
ISPICKVAL( LeadSource , &quot;Industry Event&quot;),
ISPICKVAL( LeadSource , &quot;RR Referral&quot;), 
ISPICKVAL(LeadSource,&quot;COI&quot;)
)
),
NOT(ISBLANK(Lead_Source_Detail__c )))
&amp;&amp; $Profile.Name != &apos;System Administrator&apos;</errorConditionFormula>
        <errorDisplayField>Lead_Source_Detail__c</errorDisplayField>
        <errorMessage>Please clear Lead Source detail which is required only for the Lead Source of &apos;Third Part Recruiter&apos;,&apos;Industry Event&apos; and &apos;COI&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LeadSourceDetail_Required</fullName>
        <active>false</active>
        <description>Based on LeadSource Picklist value leadSourceDetail text field Should be enter.</description>
        <errorConditionFormula>AND(OR(
ISPICKVAL( LeadSource , &quot;Third Party Recruiter&quot;),
ISPICKVAL( LeadSource , &quot;Industry Event&quot;),
ISPICKVAL( LeadSource , &quot;RR Referral&quot;), 
ISPICKVAL(LeadSource,&quot;COI&quot;) 
) , 
ISBLANK(Lead_Source_Detail__c ))
&amp;&amp; $Profile.Name != &apos;System Administrator&apos;</errorConditionFormula>
        <errorDisplayField>Lead_Source_Detail__c</errorDisplayField>
        <errorMessage>Lead Source Detail is required for Lead Source of &apos;Third Part Recruiter&apos;,&apos;Industry Event&apos; and &apos;COI&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NonEditing_Closed_Opportunity</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
      $Profile.Name &lt;&gt; &apos;System Administrator&apos;, 
      $Profile.Name &lt;&gt; &apos;Business Owner&apos;,
      RecordType.DeveloperName =&apos;ReadOnly_Opportunity&apos;,
      NOT(ISNEW()),
      PRIORVALUE( IsClosed ) = True,
      OR (ISCHANGED( CloseDate ),ISCHANGED(StageName)) 
)</errorConditionFormula>
        <errorMessage>Closed Opportunities are not editable</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OfferAcceptedAndDeclinedCantbetogether</fullName>
        <active>true</active>
        <description>OfferAcceptedAndDeclinedCantbetogether</description>
        <errorConditionFormula>NOT(ISBLANK(  Offer_Accepted__c ))  &amp;&amp;   NOT(ISBLANK(   Offer_Declined__c  ))</errorConditionFormula>
        <errorMessage>Offer Accepted and Declined cannot be set together</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OpportunityEditPermission</fullName>
        <active>false</active>
        <description>Opportunity Edit Permission</description>
        <errorConditionFormula>AND( NOT(ISNEW()), 
     OR($Profile.Name  &lt;&gt;&apos;Business Owner&apos;,
        $Profile.Name  &lt;&gt;&apos;System Administrator&apos;),        ISCHANGED(LastModifiedDate)
)</errorConditionFormula>
        <errorMessage>Only Business Owner or System Administrator  can edit opportunity record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Name_should_be_read_Only</fullName>
        <active>true</active>
        <description>Opportunity_Name_should_be_read_Only</description>
        <errorConditionFormula>AND(
     AND($Profile.Name  &lt;&gt; &apos;System Administrator&apos;, 
         $Profile.Name  &lt;&gt; &apos;Business Owner&apos;)
,  
ISCHANGED( Name ))</errorConditionFormula>
        <errorMessage>Opportunity Name can not be edited</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prospect_Jobtitle_is_not_Registered_Rep</fullName>
        <active>true</active>
        <description>User can&apos;t edit Opportunity if opportunity-prospect jobtitle is not registered rep</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &apos;System Administrator&apos;, 
$Profile.Name &lt;&gt; &apos;System Administrator - ETL&apos;,
ISCHANGED( Initiate_Transition_Communication__c ), 
NOT(
    AND( $Profile.Name ==&apos;Transition&apos;,
         ISPICKVAL(Contact__r.Job_Title__c, &apos;Registered Rep&apos;)
        )
    )
)</errorConditionFormula>
        <errorMessage>Initiate Transition Communication can not be edited for this record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Stage_Name_read_Only_for_Recruitor</fullName>
        <active>true</active>
        <description>Stage_Name_read_Only_for_Recruitor</description>
        <errorConditionFormula>AND(
    OR(
        $Profile.Name = &apos;Director of Recruiting&apos;, 
        $Profile.Name = &apos;Managing Director&apos;,
        $Profile.Name = &apos;Managing Principal&apos;,
        $Profile.Name = &apos;National Recruiter&apos;
      ),  
    OR(
        ISCHANGED( Name ) 
        
      )
   )
&amp;&amp; $Profile.Name != &apos;System Administrator&apos;</errorConditionFormula>
        <errorMessage>Recruiters are not eligible to edit opportunity fields except Prelim. App Sent</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Transition_Opt_Out_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   $Profile.Name &lt;&gt; &apos;System Administrator&apos;, 
   $Profile.Name &lt;&gt; &apos;System Administrator - ETL&apos;,
   ISCHANGED(Transition_Opt_Out__c), 
   $Profile.Name !=&apos;Transition&apos;
)</errorConditionFormula>
        <errorMessage>Only Transition User can set Transition Opt Out</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TypeIsTransitionValidation</fullName>
        <active>true</active>
        <description>This rule validates that only home office profiles can choose Type field value as &apos;Transition Service&apos;</description>
        <errorConditionFormula>AND(
$Profile.Name!=&apos;System Administrator&apos;,
$Profile.Name!=&apos;Business Owner&apos;,
$Profile.Name!=&apos;Compliance&apos;,
$Profile.Name!=&apos;System Administrator - API&apos;,
$Profile.Name!=&apos;System Support&apos;,
$Profile.Name!=&apos;Transition&apos;,
ISPICKVAL(Type, &apos;Transition Services&apos;)
)</errorConditionFormula>
        <errorDisplayField>Type</errorDisplayField>
        <errorMessage>&apos;Transition Services&apos; is for Home Office use only.</errorMessage>
    </validationRules>
