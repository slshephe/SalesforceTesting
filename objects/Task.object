    <validationRules>
        <fullName>Cannot_Change_Owner_on_Eloqua_Tasks</fullName>
        <active>true</active>
        <description>No one can change the assigned to on Eloqua Tasks</description>
        <errorConditionFormula>AND (RecordType.DeveloperName = &apos;Eloqua_Task&apos;, ISCHANGED ( OwnerId),  $Profile.Name != &apos;API - Eloqua&apos;,$Profile.Name != &apos;System Administrator&apos;)</errorConditionFormula>
        <errorMessage>You cannot change the activity owner</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prospect_is_Required_for_Opty_Task</fullName>
        <active>false</active>
        <description>To ensure that Prospect (WhoId) is selected for an Task.</description>
        <errorConditionFormula>AND($Profile.Name  &lt;&gt; &apos;System Administrator&apos;, $Profile.Name  &lt;&gt; &apos;Business Owner&apos;,ISBLANK( WhoId ))</errorConditionFormula>
        <errorDisplayField>WhoId</errorDisplayField>
        <errorMessage>Lead/Prospect is Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValidateResult</fullName>
        <active>true</active>
        <description>Rule to ensure result is populated on completing the task</description>
        <errorConditionFormula>ISPICKVAL(Status,&apos;Completed&apos;) &amp;&amp; 
ISPICKVAL(Result__c, &quot;&quot;) &amp;&amp; 
NOT ISPICKVAL(Activity__c ,&quot;&quot;) &amp;&amp; 
NOT ISPICKVAL(Activity__c ,&quot;To-Do&quot;) &amp;&amp; 
NOT ISPICKVAL(Activity__c ,&quot;Email&quot;) &amp;&amp; 
NOT ISPICKVAL(Purpose__c, &quot;&quot;)
&amp;&amp; $Profile.Name != &apos;System Administrator&apos;</errorConditionFormula>
        <errorMessage>Please enter a Result when changing the status to Completed.</errorMessage>
    </validationRules>
